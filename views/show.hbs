    <h2>{{title}}</h2>

    <h3>Description:</h3>
    <p>{{description}}</p>

    <h3>Ingredients:</h3>
    <ul>
        {{#each this.ingredients}}
        <li>{{this}}</li>
        {{/each}}
    </ul>

    <h3>Instruction:</h3>
    <ul>
        {{#each this.instruction}}
        <li>{{this}}</li>
        {{/each}}
    </ul>

    
